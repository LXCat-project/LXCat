{
  "plugins": {
    "postcss-flexbugs-fixes": {},
    "postcss-simple-vars": {
      "variables": {
        "mantine-breakpoint-xs": "36em",
        "mantine-breakpoint-sm": "48em",
        "mantine-breakpoint-md": "62em",
        "mantine-breakpoint-lg": "75em",
        "mantine-breakpoint-xl": "88em"
      }
    },
    "postcss-preset-mantine": {},
    "postcss-preset-env": {
      "autoprefixer": {
        "flexbox": "no-2009"
      },
      "stage": 3,
      "features": {
        "custom-properties": false,
        "nesting-rules": true
      }
    }
  }
}
