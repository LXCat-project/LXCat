{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "format": { "cache": false },
    "lint": {},
    "clean": { "cache": false },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**", ".next/**", "target/**"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "target/**"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "test": {},
    "annotate": { "cache": false }
  }
}
