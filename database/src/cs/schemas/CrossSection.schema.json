{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "definitions": {},
  "properties": {
    "data": {
      "items": {
        "items": {
          "type": "number"
        },
        "maxItems": 2,
        "minItems": 2,
        "type": "array"
      },
      "type": "array"
    },
    "labels": {
      "items": {
        "type": "string"
      },
      "maxItems": 2,
      "minItems": 2,
      "type": "array"
    },
    "organization": {
      "type": "string"
    },
    "parameters": {
      "additionalProperties": false,
      "properties": {
        "mass_ratio": {
          "type": "number"
        },
        "statistical_weight_ratio": {
          "type": "number"
        }
      },
      "type": "object"
    },
    "reaction": {
      "type": "string"
    },
    "threshold": {
      "type": "number"
    },
    "type": {
      "const": "LUT",
      "type": "string"
    },
    "units": {
      "items": {
        "type": "string"
      },
      "maxItems": 2,
      "minItems": 2,
      "type": "array"
    },
    "versionInfo": {
      "additionalProperties": false,
      "properties": {
        "commitMessage": {
          "type": "string"
        },
        "createdOn": {
          "type": "string"
        },
        "retractMessage": {
          "type": "string"
        },
        "status": {
          "description": "Valid transitions:\n* published\n* draft -> published + published -> archived (draft of published set gets published and published set gets archived)\n* published -> retracted",
          "enum": ["draft", "published", "archived", "retracted"],
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "required": ["status", "version", "createdOn"],
      "type": "object"
    }
  },
  "required": [
    "data",
    "labels",
    "organization",
    "reaction",
    "threshold",
    "type",
    "units",
    "versionInfo"
  ],
  "type": "object"
}
