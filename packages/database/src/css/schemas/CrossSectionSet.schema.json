{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "definitions": {},
  "properties": {
    "complete": {
      "type": "boolean"
    },
    "description": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "organization": {
      "type": "string"
    },
    "versionInfo": {
      "additionalProperties": false,
      "properties": {
        "commitMessage": {
          "type": "string"
        },
        "createdOn": {
          "type": "string"
        },
        "retractMessage": {
          "type": "string"
        },
        "status": {
          "description": "Valid transitions:\n* published\n* draft -> published + published -> archived (draft of published set gets published and published set gets archived)\n* published -> retracted",
          "enum": ["draft", "published", "archived", "retracted"],
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "required": ["status", "version", "createdOn"],
      "type": "object"
    }
  },
  "required": [
    "name",
    "description",
    "complete",
    "organization",
    "versionInfo"
  ],
  "type": "object"
}
